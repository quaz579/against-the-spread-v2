{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "http": {
            "routePrefix": "api"
        }
    },
    "host": {
        "cors": {
            "allowedOrigins": [
                "http://localhost:5158",
                "https://localhost:5158"
            ],
            "supportCredentials": false
        }
    }
}